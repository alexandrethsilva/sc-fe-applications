{
  "presets": [
    [
      "env",
      {
        "debug": true,
        "targets": {
          "browsers": "last 2 versions",
          "node": "current",
          "uglify": false
        },
        "modules": "commonjs",
        "useBuiltIns": false,
        "production": {
          "presets": [
            "babili"
          ]
        }
      }
    ],
    "react",
    "flow"
  ],
  "plugins": [
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ]
  ],
  "retainLines": true
}